	option name 'BV_00'
	option alarm_value '0'
	option tagname 'modbus-s1'
	option addr '5'
	option bit '0'
	option resolution 'dword'
	option active 'Ein'
	option inactive 'Aus'
	option description 'Datenwort 2 Bit 0'
	option value '0'
	option Out_Of_Service '0'
	option value_time '1384274334'

